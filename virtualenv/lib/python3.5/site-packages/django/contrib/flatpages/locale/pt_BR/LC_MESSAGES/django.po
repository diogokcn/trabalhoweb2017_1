#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/admin.py:11
msgid "Advanced options"
msgstr "Opções avançadas"

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/apps.py:7
msgid "Flat Pages"
msgstr "Páginas Planas"

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/forms.py:8
#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/models.py:12
msgid "URL"
msgstr "URL"

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/forms.py:9
msgid ""
"Example: '/about/contact/'. Make sure to have leading and trailing slashes."
msgstr "Exemplo: '/sobre/contato/'. Lembre-se das barras no começo e no final."

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/forms.py:12
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Este valor deve conter apenas letras, números, pontos, sublinhados, traços, "
"barras ou til."

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/forms.py:25
msgid "URL is missing a leading slash."
msgstr "Está faltando uma barra no início da URL."

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/forms.py:32
msgid "URL is missing a trailing slash."
msgstr "Está faltando uma barra no final da URL."

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/forms.py:49
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "Uma flatpage com a URL %(url)s já existe para o site %(site)s"

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/models.py:13
msgid "title"
msgstr "título"

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/models.py:14
msgid "content"
msgstr "conteúdo"

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/models.py:15
msgid "enable comments"
msgstr "habilitar comentários"

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/models.py:16
msgid "template name"
msgstr "nome do template"

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/models.py:18
msgid ""
"Example: 'flatpages/contact_page.html'. If this isn't provided, the system "
"will use 'flatpages/default.html'."
msgstr ""
"Exemplo: 'flatpages/contact_page.html'. Se não for informado, será utilizado "
"'flatpages/default.html'."

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/models.py:22
msgid "registration required"
msgstr "registro obrigatório"

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/models.py:23
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"Se estiver assinalado, apenas usuários autenticados poderão ver a página."

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/models.py:29
msgid "flat page"
msgstr "página plana"

#: virtualenv/lib/python3.5/site-packages/django/contrib/flatpages/models.py:30
msgid "flat pages"
msgstr "páginas planas"
